{
	"go.toolsEnvVars": {
		"GOOS": "js",
		"GOARCH": "wasm"
	},
	"eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "svelte"],
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
